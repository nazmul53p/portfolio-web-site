{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "This site all about Nazmul Haque Details.",
  "main": "index.js",
  "repository": "https://github.com/nazmul53p/portfolio-web-site.git",
  "author": "nazmul",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/index.ts",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rotating-file-stream": "^3.0.4",
    "sequelize": "^6.21.2",
    "sequelize-typescript": "^2.1.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sequelize": "^4.28.13",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-typescript-prettier"
    ],
    "plugins": [
      "import",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "@typescript-eslint/no-unused-vars": 1,
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "none",
          "singleQuote": true,
          "printWidth": 100,
          "tabWidth": 4
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    }
  }
}
